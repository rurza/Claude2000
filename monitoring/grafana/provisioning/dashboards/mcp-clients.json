{
  "dashboard": {
    "id": null,
    "uid": "continuous-claude-mcp",
    "title": "Continuous-Claude-v3 MCP Clients",
    "description": "MCP (Model Context Protocol) client connections, tool calls, and performance metrics",
    "tags": ["continuous-claude", "mcp", "integration", "tools"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "MCP Servers Connected",
        "type": "stat",
        "gridPos": {"x": 0, "y": 0, "w": 4, "h": 3},
        "targets": [
          {
            "expr": "mcp_servers_connected",
            "legendFormat": "Connected"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "green", "value": 3}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Active Connections",
        "type": "stat",
        "gridPos": {"x": 4, "y": 0, "w": 4, "h": 3},
        "targets": [
          {
            "expr": "mcp_active_connections",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "green", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Total Tool Calls (1h)",
        "type": "stat",
        "gridPos": {"x": 8, "y": 0, "w": 4, "h": 3},
        "targets": [
          {
            "expr": "increase(mcp_tool_calls_total[1h])",
            "legendFormat": "Calls"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1000},
                {"color": "red", "value": 10000}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Success Rate (1h)",
        "type": "stat",
        "gridPos": {"x": 12, "y": 0, "w": 4, "h": 3},
        "targets": [
          {
            "expr": "increase(mcp_tool_calls_total{status=\"success\"}[1h]) / (increase(mcp_tool_calls_total{status=\"success\"}[1h]) + increase(mcp_tool_calls_total{status=\"error\"}[1h])) * 100",
            "legendFormat": "Success Rate"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 99}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Connection State Distribution",
        "type": "piechart",
        "gridPos": {"x": 16, "y": 0, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "sum by (server_name, transport) (mcp_connection_state)",
            "legendFormat": "{{server_name}} ({{transport}})"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 6,
        "title": "Tool Call Rate by Server",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 3, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "rate(mcp_tool_calls_total{status=\"success\"}[1m]) * 60",
            "legendFormat": "{{server_name}} {{tool_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/min",
            "custom": {
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Tool Call Latency p50",
        "type": "timeseries",
        "gridPos": {"x": 8, "y": 3, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(mcp_tool_latency_seconds_bucket[5m]))",
            "legendFormat": "{{server_name}} {{tool_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 2}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Tool Call Latency p95",
        "type": "timeseries",
        "gridPos": {"x": 16, "y": 3, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(mcp_tool_latency_seconds_bucket[5m]))",
            "legendFormat": "{{server_name}} {{tool_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Tool Call Latency p99",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 7, "w": 12, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.99, rate(mcp_tool_latency_seconds_bucket[5m]))",
            "legendFormat": "{{server_name}} {{tool_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 2},
                {"color": "red", "value": 10}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Tool Call Status",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 7, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(mcp_tool_calls_total{status=\"success\"}[5m])",
            "legendFormat": "{{server_name}} success"
          },
          {
            "expr": "rate(mcp_tool_calls_total{status=\"error\"}[5m])",
            "legendFormat": "{{server_name}} error"
          },
          {
            "expr": "rate(mcp_tool_calls_total{status=\"retry\"}[5m])",
            "legendFormat": "{{server_name}} retry"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 0.01}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Error Rate by Server",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 7, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(mcp_tool_calls_total{status=\"error\"}[5m]) / rate(mcp_tool_calls_total[5m])",
            "legendFormat": "{{server_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.05},
                {"color": "red", "value": 0.1}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Tools Available per Server",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 11, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "mcp_tools_available",
            "legendFormat": "{{server_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Cache Hit Rate",
        "type": "timeseries",
        "gridPos": {"x": 6, "y": 11, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(mcp_cache_hits_total[5m]) / (rate(mcp_cache_hits_total[5m]) + rate(mcp_cache_misses_total[5m]))",
            "legendFormat": "{{server_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 0.8}
              ]
            }
          }
        }
      },
      {
        "id": 14,
        "title": "Cache Hits vs Misses",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 11, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(mcp_cache_hits_total[5m])",
            "legendFormat": "{{server_name}} hits"
          },
          {
            "expr": "rate(mcp_cache_misses_total[5m])",
            "legendFormat": "{{server_name}} misses"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 15,
        "title": "Retry Count",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 11, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(mcp_retries_total[5m])",
            "legendFormat": "{{server_name}} {{tool_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        }
      },
      {
        "id": 16,
        "title": "Connection Latency by Transport",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 15, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(mcp_connection_latency_seconds_bucket[5m]))",
            "legendFormat": "{{server_name}} {{transport}} p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(mcp_connection_latency_seconds_bucket[5m]))",
            "legendFormat": "{{server_name}} {{transport}} p95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 30}
              ]
            }
          }
        }
      },
      {
        "id": 17,
        "title": "Top Tools by Call Volume",
        "type": "barchart",
        "gridPos": {"x": 8, "y": 15, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "topk(10, sum by (tool_name) (increase(mcp_tool_calls_total[1h])))",
            "legendFormat": "{{tool_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 18,
        "title": "Slowest Tools (p99 Latency)",
        "type": "barchart",
        "gridPos": {"x": 16, "y": 15, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "topk(10, histogram_quantile(0.99, rate(mcp_tool_latency_seconds_bucket[1h])))",
            "legendFormat": "{{server_name}} {{tool_name}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {"type": "grafana", "uid": "-- Grafana --"},
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "current": {"selected": false, "text": "All", "value": "$__all"},
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "definition": "label_values(mcp_connection_state, server_name)",
          "hide": 0,
          "includeAll": true,
          "label": "Server",
          "multi": true,
          "name": "server",
          "options": [],
          "query": "label_values(mcp_connection_state, server_name)",
          "refresh": 2,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query"
        },
        {
          "current": {"selected": false, "text": "All", "value": "$__all"},
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "definition": "label_values(mcp_tool_calls_total, tool_name)",
          "hide": 0,
          "includeAll": true,
          "label": "Tool",
          "multi": true,
          "name": "tool",
          "options": [],
          "query": "label_values(mcp_tool_calls_total, tool_name)",
          "refresh": 2,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query"
        },
        {
          "current": {"selected": false, "text": "All", "value": "$__all"},
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "definition": "label_values(mcp_connection_state, transport)",
          "hide": 0,
          "includeAll": true,
          "label": "Transport",
          "multi": true,
          "name": "transport",
          "options": [],
          "query": "label_values(mcp_connection_state, transport)",
          "refresh": 2,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query"
        },
        {
          "current": {"selected": false, "text": "All", "value": "$__all"},
          "datasource": {"type": "prometheus", "uid": "prometheus"},
          "definition": "label_values(mcp_tool_calls_total, status)",
          "hide": 0,
          "includeAll": true,
          "label": "Status",
          "multi": true,
          "name": "status",
          "options": [],
          "query": "label_values(mcp_tool_calls_total, status)",
          "refresh": 2,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query"
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "links": [
      {
        "title": "Overview Dashboard",
        "type": "link",
        "url": "/d/continuous-claude-overview/overview",
        "icon": "home",
        "targetBlank": false
      },
      {
        "title": "Stream Monitoring Dashboard",
        "type": "link",
        "url": "/d/continuous-claude-stream/stream-monitoring",
        "icon": "stream",
        "targetBlank": false
      },
      {
        "title": "Performance Dashboard",
        "type": "link",
        "url": "/d/continuous-claude-performance/performance",
        "icon": "chart-line",
        "targetBlank": false
      }
    ]
  },
  "overwrite": true,
  "message": "Created MCP Clients dashboard"
}
