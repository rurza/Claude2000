{
  "dashboard": {
    "id": null,
    "uid": "continuous-claude-memory",
    "title": "Continuous-Claude-v3 Memory System",
    "description": "Learning pipeline monitoring: extractions, recall, and store operations",
    "tags": ["continuous-claude", "memory", "learning", "extraction"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Extractions Rate (per min)",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(memory_daemon_extractions_total[1m])",
            "legendFormat": "{{session_id}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/min",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 20}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Extraction Status",
        "type": "piechart",
        "gridPos": {"x": 6, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "increase(memory_daemon_extraction_duration_seconds_count[1h])",
            "legendFormat": "{{status}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "mappings": []
          }
        }
      },
      {
        "id": 3,
        "title": "Extraction Duration",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(memory_daemon_extraction_duration_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(memory_daemon_extraction_duration_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(memory_daemon_extraction_duration_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "red", "value": 30}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Queue Backlog",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 0, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "memory_daemon_queue_depth",
            "legendFormat": "Queue Depth"
          },
          {
            "expr": "memory_daemon_active_extractions",
            "legendFormat": "Active"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 50},
                {"color": "red", "value": 100}
              ]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Recall Query Latency",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 4, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(recall_query_latency_seconds_bucket[5m]))",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(recall_query_latency_seconds_bucket[5m]))",
            "legendFormat": "p95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(recall_query_latency_seconds_bucket[5m]))",
            "legendFormat": "p99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 2}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Recall Queries by Backend",
        "type": "timeseries",
        "gridPos": {"x": 8, "y": 4, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "rate(recall_queries_total{backend=\"postgres\"}[5m])",
            "legendFormat": "PostgreSQL"
          },
          {
            "expr": "rate(recall_queries_total{backend=\"sqlite\"}[5m])",
            "legendFormat": "SQLite"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Recall Search Type Distribution",
        "type": "piechart",
        "gridPos": {"x": 16, "y": 4, "w": 8, "h": 4},
        "targets": [
          {
            "expr": "sum by (search_type) (rate(recall_queries_total[1h]))",
            "legendFormat": "{{search_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 8,
        "title": "Cache Hit Rate",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(recall_cache_hits_total[5m]) / (rate(recall_cache_hits_total[5m]) + rate(recall_cache_misses_total[5m]))",
            "legendFormat": "{{cache_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "green", "value": 0.8}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Cache Hits vs Misses",
        "type": "timeseries",
        "gridPos": {"x": 6, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(recall_cache_hits_total[5m])",
            "legendFormat": "Hits {{cache_type}}"
          },
          {
            "expr": "rate(recall_cache_misses_total[5m])",
            "legendFormat": "Misses {{cache_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Store Operations by Status",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(store_operations_total{status=\"success\"}[5m])",
            "legendFormat": "Success"
          },
          {
            "expr": "rate(store_operations_total{status=\"error\"}[5m])",
            "legendFormat": "Error"
          },
          {
            "expr": "rate(store_operations_total{status=\"skipped\"}[5m])",
            "legendFormat": "Skipped"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 11,
        "title": "Store Latency by Phase",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 8, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(store_latency_seconds_bucket[5m]))",
            "legendFormat": "{{phase}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 12,
        "title": "Deduplication Rate",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 12, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(store_deduplication_skipped_total[5m])",
            "legendFormat": "{{reason}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 13,
        "title": "Learning Types Distribution",
        "type": "barchart",
        "gridPos": {"x": 6, "y": 12, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum by (type) (increase(store_learning_types_total[1h]))",
            "legendFormat": "{{type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 14,
        "title": "Confidence Levels",
        "type": "piechart",
        "gridPos": {"x": 12, "y": 12, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum by (level) (increase(store_confidence_levels_total[1h]))",
            "legendFormat": "{{level}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        }
      },
      {
        "id": 15,
        "title": "Embedding Latency",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 12, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(recall_embedding_latency_seconds_bucket[5m]))",
            "legendFormat": "recall p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(recall_embedding_latency_seconds_bucket[5m]))",
            "legendFormat": "recall p95"
          },
          {
            "expr": "histogram_quantile(0.50, rate(store_embedding_time_seconds_bucket[5m]))",
            "legendFormat": "store p50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(store_embedding_time_seconds_bucket[5m]))",
            "legendFormat": "store p95"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "thresholdsStyle": {"mode": "line"}
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.5},
                {"color": "red", "value": 2}
              ]
            }
          }
        }
      },
      {
        "id": 16,
        "title": "Backend Switches",
        "type": "timeseries",
        "gridPos": {"x": 0, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(recall_backend_switches_total[5m])",
            "legendFormat": "{{from_backend}} -> {{to_backend}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.1},
                {"color": "red", "value": 0.5}
              ]
            }
          }
        }
      },
      {
        "id": 17,
        "title": "Results Returned Distribution",
        "type": "histogram",
        "gridPos": {"x": 6, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(recall_results_returned_bucket[5m]))",
            "legendFormat": "{{backend}} {{search_type}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "lineWidth": 1
            }
          }
        }
      },
      {
        "id": 18,
        "title": "Stale Sessions Found",
        "type": "timeseries",
        "gridPos": {"x": 12, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(memory_daemon_stale_sessions_found[5m])",
            "legendFormat": "Stale sessions"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            }
          }
        }
      },
      {
        "id": 19,
        "title": "Error Rate by Type",
        "type": "timeseries",
        "gridPos": {"x": 18, "y": 16, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "rate(store_operations_total{status=\"error\"}[5m])",
            "legendFormat": "store {{backend}}"
          },
          {
            "expr": "rate(recall_query_errors_total[5m])",
            "legendFormat": "recall errors"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops/s",
            "custom": {
              "fillOpacity": 10
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.1}
              ]
            }
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": {"type": "grafana", "uid": "-- Grafana --"},
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
